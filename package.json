{
  "name": "nt-composer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "compile": "npx babel-node ./compile/index.js",
    "preview": "npx babel-node ./preview/server.js",
    "watch:edit": "npx nodemon -x \"npm run compile\" -w ./resources -e \"yml,png,jpg\"",
    "launch:browser": "open http://localhost:9999",
    "dev": "concurrently --kill-others \"npm run preview\" \"npm run watch:edit\"",
    "start": "npm run launch:browser && npm run dev"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "SuperCanvas": "https://github.com/NickBeeuwsaert/SuperCanvas",
    "babel-preset-env": "^1.7.0",
    "bezier-js": "^2.6.1",
    "concurrently": "^5.2.0",
    "fs-extra": "^9.0.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "object-hash": "^2.0.3",
    "omgsvg": "https://github.com/deanm/omgsvg",
    "parcel-bundler": "^1.12.4",
    "spritesmith": "^3.4.0",
    "stats.js": "^0.17.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "express": "^4.17.1",
    "http-server": "^0.12.3",
    "nodemon": "^2.0.3",
